Algoritmo "NumeroMaior"
Var
  num1, num2: inteiro
Inicio
      escreval ("Digite o primeiro valor: ")
      leia(num1)
      escreval ("Digite o segundo valor: ")
      leia(num2)
      Se (num1 > num2) entao
         escreval ("O primeiro número é maior: ",num1)
      fimse
      se (num2 > num1) entao
         escreval ("O segundo número é maior: ",num2)
      fimse
Fimalgoritmo