Algoritmo "NumeroNegativoOuPositivo"
Var
   num: real
Inicio
      escreval ("Digite um valor: ")
      leia(num)
      se (num>0) entao
         escreval ("O valor digitado é positivo!")
      fimse
      se (num<0) entao
         escreval ("O valor digitado é negativo!")
      fimse
      se (num=0) entao
         escreval ("O valor digitado é zero!")
      fimse
Fimalgoritmo