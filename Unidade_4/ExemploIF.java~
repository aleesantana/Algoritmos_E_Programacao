public class ExemploIF
{
  public static void main (String args[])
  {
    int nota = 5;
    if (nota >=5 && nota <=6) {
      System.out.println("Aluno em recuperação.");
    }
    else
    {
      if (nota < 5 )
      {
        System.out.println("Reprovado");
      }
      if (nota > 6)
      {
        System.out.println("Aprovado.");
      }
    }
  }
}